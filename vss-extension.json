{
  "manifestVersion": 1,
  "id": "bane-extension-package",
  "name": "Bane Build and Release Tools",
  "version": "0.0.1",
  "publisher": "bane",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tools for building/releasing with Bane. Includes one build/release task.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "buildandreleasetask"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildandreleasetask"
      }
    }
  ]
}